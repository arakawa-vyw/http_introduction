<!DOCTYPE html>
<html>
  <head>
    <title>Http</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <%= render_breadcrumbs %>

    <%= yield %>

    <% unless @non_request_info %>
      <%= render "share/request_info" %>
    <% end %>

    <br/>
    <br/>
  </body>
</html>
